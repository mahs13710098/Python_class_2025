    "def movsefr(lst1):\n",
    "    zero_counts = lst1.count(0)\n",
    "    for _ in range(zero_counts):\n",
    "        lst1.remove(0)\n",
    "    lst1.extend([0 for _ in range(zero_counts)])\n",
    "    return lst1\n"
