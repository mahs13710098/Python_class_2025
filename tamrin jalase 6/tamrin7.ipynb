   "source": [
    "def reverse_number(n):\n",
    "    s = str(n)\n",
    "    if s[0].isdigit():\n",
    "          reversed_s = s[::-1]  \n",
    "    else:\n",
    "          reversed_s = s[0] +s[:0:-1]\n",
    "    reversed_num = int(reversed_s)\n",
    "    \n",
    "    return reversed_num"
   ]
