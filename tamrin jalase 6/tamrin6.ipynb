    "def max_value(lst):\n",
    "    lst2=[]\n",
    "    for item in lst:\n",
    "        if isinstance(item,str):\n",
    "            lst2.append(len(item))\n",
    "        else:\n",
    "            lst2.append(item)\n",
    "    return max(lst2)\n"
   ]
